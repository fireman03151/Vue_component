{
  "name": "ant-design-vue",
  "version": "2.0.0-rc.7",
  "title": "Ant Design Vue",
  "description": "An enterprise-class UI design language and Vue-based implementation",
  "keywords": [
    "vue",
    "vue3",
    "ant",
    "design",
    "antd",
    "vueComponent",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
